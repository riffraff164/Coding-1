++const crypto = require('crypto');
++
++module.exports = {
++  sortObj: function(obj) {
++    let keys = Object.keys(obj).sort(),
++      sortedObj = {};
++    for (let i in keys) {
++      sortedObj[keys[i]] = obj[keys[i]];
++    }
++    return sortedObj;
++  },
++
++  randBetween: function(min, max) {
